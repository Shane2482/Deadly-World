		C3.addBox(0F, 0F, 0F, 4, 1, 1);
		C3.setRotationPoint(-2F, 8F, -4F);
		C3.setTextureSize(64, 64);
		C3.mirror = true;
		setRotation(C3, 0F, 0F, 0F);
		C13 = new ModelRenderer(this, 31, 0);
		C13.addBox(0F, 0F, 0F, 1, 1, 4);
		C13.setRotationPoint(-5F, 8F, -1F);
		C13.setTextureSize(64, 64);
		C13.mirror = true;
		setRotation(C13, 0F, 0F, 0F);
		C3 = new ModelRenderer(this, 15, 0);
		C3.addBox(0F, 0F, 0F, 4, 1, 1);
