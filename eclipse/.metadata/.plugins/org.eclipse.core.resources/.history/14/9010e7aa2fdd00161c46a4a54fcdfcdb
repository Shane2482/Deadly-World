
package shane2482.deadlyworld.model;

import net.minecraft.client.model.ModelBase;
import net.minecraft.client.model.ModelRenderer;
import net.minecraft.entity.Entity;

public class furnace_model extends ModelBase {
	// fields
	ModelRenderer base;
	ModelRenderer Flame;
	ModelRenderer Ft;
	ModelRenderer FC;
	ModelRenderer FR;
	ModelRenderer FB;
	ModelRenderer C2;
	ModelRenderer C5;
	ModelRenderer C8;
	ModelRenderer C10;
	ModelRenderer FS;
	ModelRenderer C11;
	ModelRenderer C12;
	ModelRenderer C13;
	ModelRenderer C3;
	ModelRenderer FL;
	ModelRenderer FM;
	ModelRenderer C4;
	ModelRenderer C1;
	ModelRenderer FCL;

	public furnace_model() {
		textureWidth = 64;
		textureHeight = 64;

		base = new ModelRenderer(this, 0, 0);
		base.addBox(0F, 0F, 0F, 16, 15, 15);
		base.setRotationPoint(-8F, 9F, -7F);
		base.setTextureSize(64, 64);
		base.mirror = true;
		setRotation(base, 0F, 0F, 0F);
		Flame = new ModelRenderer(this, 0, 32);
		Flame.addBox(0F, 0F, 0F, 12, 3, 0);
		Flame.setRotationPoint(-6F, 18F, -7.5F);
		Flame.setTextureSize(64, 64);
		Flame.mirror = true;
		setRotation(Flame, 0F, 0F, 0F);
		Ft = new ModelRenderer(this, 28, 28);
		Ft.addBox(0F, 0F, 0F, 16, 2, 1);
		Ft.setRotationPoint(-8F, 9F, -8F);
		Ft.setTextureSize(64, 64);
		Ft.mirror = true;
		setRotation(Ft, 0F, 0F, 0F);
		FCL = new ModelRenderer(this, 0, 15);
		FCL.addBox(0F, 0F, 0F, 3, 4, 1);
		FCL.setRotationPoint(-6F, 11F, -8F);
		FCL.setTextureSize(64, 64);
		FCL.mirror = true;
		setRotation(FCL, 0F, 0F, 0F);
		FR = new ModelRenderer(this, 0, 15);
		FR.addBox(0F, 0F, 0F, 1, 10, 1);
		FR.setRotationPoint(-7F, 11F, -8F);
		FR.setTextureSize(64, 64);
		FR.mirror = true;
		setRotation(FR, 0F, 0F, 0F);
		FB = new ModelRenderer(this, 15, 0);
		FB.addBox(0F, 0F, 0F, 14, 3, 1);
		FB.setRotationPoint(-7F, 21F, -8F);
		FB.setTextureSize(64, 64);
		FB.mirror = true;
		setRotation(FB, 0F, 0F, 0F);
		C2 = new ModelRenderer(this, 15, 0);
		C2.addBox(0F, 0F, 0F, 1, 1, 1);
		C2.setRotationPoint(2F, 14F, -8F);
		C2.setTextureSize(64, 64);
		C2.mirror = true;
		setRotation(C2, 0F, 0F, 0F);
		C5 = new ModelRenderer(this, 15, 0);
		C5.addBox(0F, 0F, 0F, 1, 1, 1);
		C5.setRotationPoint(-3F, 14F, -8F);
		C5.setTextureSize(64, 64);
		C5.mirror = true;
		setRotation(C5, 0F, 0F, 0F);
		C8 = new ModelRenderer(this, 15, 0);
		C8.addBox(0F, 0F, 0F, 2, 1, 1);
		C8.setRotationPoint(-6F, 18F, -8F);
		C8.setTextureSize(64, 64);
		C8.mirror = true;
		setRotation(C8, 0F, 0F, 0F);
		C10 = new ModelRenderer(this, 15, 0);
		C10.addBox(0F, 0F, 0F, 2, 1, 1);
		C10.setRotationPoint(4F, 18F, -8F);
		C10.setTextureSize(64, 64);
		C10.mirror = true;
		setRotation(C10, 0F, 0F, 0F);
